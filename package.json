{
  "name": "@daniel-ordonez/vue-chatui",
  "version": "0.3.0",
  "private": false,
  "description": "A Vuejs component for creating conversational UIs based on simple 2-way chat.",
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel-ordonez/vue-chatui",
    "homepage": "https://github.com/daniel-ordonez/vue-chatui#readme"
  },
  "license": "GFDL-1.3-or-later",
  "keywords": [
    "Vuejs",
    "Vue component",
    "Chat",
    "Conversational UI"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-vue": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name VueChatui ./src/components/Chatui.vue",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/VueChatui.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "screenshots/*"
  ],
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
